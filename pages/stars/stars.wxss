.fansContainer{
  width: 100%;
  height: 80px;
  background-color: #FFFEF9;
  display: flex;
  flex-direction: row;
  align-items: flex-start;
  margin-top: 1px;
}

.userIcon{
  width: 35px;
  height: 35px;
  border-radius: 50%;
  margin-top: 15px;
  margin-left: 15px;
  margin-right: 20px;
  background-color: white;
}

.userNickname{
  width: 120px;
  height: 20px;
  background-color: #FFFEF9;
  text-align: center;
  margin-top: 15px;
}

.starView{
  width: 75px;
  height: 25px;
  background-color: #FFFEF9;
  border-radius: 10%;
  text-align: center;
  margin-left: 150rpx;
  align-self: center;
  color: #99CCE2;
  border: solid;
  border-width: 1rpx;
  border-color: #99CCE2;
}

.starViewed{
  width: 75px;
  height: 25px;
  background-color: #FFFEF9;
  border-radius: 10%;
  text-align: center;
  margin-left: 150rpx;
  align-self: center;
  color: #9F9F9F;
  border: solid;
  border-width: 1rpx;
  border-color: #9F9F9F;
}